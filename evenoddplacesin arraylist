import java.util.*;
class evenoddplaces_array
{
 public static void main(String args[])
{

res[5];num;
index=0;
for(i=0;i<num.size;i++)
{
if(num[i]%2==0)
{
res[index]=num[i];
index=index+2;
}
}
index=1;
for(i=0;i<num.size;i++)
{
if(num[i]%2!=0)
{
res[index]=num[i];
index=index+1;
}
}
System.out.println(" evenodd");
}
}
