import java.util.*;
class rev_stack
{
public static void main(String args[])
{
String s="aditya";
Stack<Character>s1=new Stack<Character>();

char ch[]=s.toCharArray();
Stack s1=new Stack();
for(int i=0;i<ch.length;i++)
{
s1.push(ch[i]);
}
for(int i=0;i<ch.length;i++)
{
System.out.println(s1.pop());
}
}
}
